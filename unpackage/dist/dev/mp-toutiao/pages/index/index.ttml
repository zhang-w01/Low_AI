<view class="body"><view><button data-event-opts="{{[['tap',[['hello',['$event']]]]]}}" bindtap="__e">dianshi</button><button data-event-opts="{{[['tap',[['myConfirm',['$event']]]]]}}" bindtap="__e">2222</button></view><scroll-view class="chat_body" scroll-y="true" scroll-top="{{scrollTop}}" scroll-with-animation="true"><view class="cu-chat" id="chat"><block tt:for="{{msgList}}" tt:for-item="x" tt:for-index="i" tt:key="i"><view><block tt:if="{{x.my}}"><view class="cu-item self"><view class="main"><view class="content bg-blue"><text selectable="true" user-select="true">{{x.msg}}</text></view></view><view class="avatar-wrapper"><image class="cu-avatar" src="/static/images/user.png" mode="aspectFill"></image></view></view></block><block tt:if="{{!x.my}}"><view class="cu-item"><image class="cu-avatar" src="/static/images/lawyer.png" mode="aspectFill"></image><view class="main"><view class="content bg-white"><text selectable="true" user-select="true">{{x.msg}}</text><block tt:if="{{x.radio_display}}"><checkbox-group data-event-opts="{{[['change',[['selectItem',['$event']]]]]}}" bindchange="__e"><block tt:for="{{radioList}}" tt:for-item="item" tt:for-index="i" tt:key="i"><view style="margin-top:10px;"><checkbox value="{{item.labtext}}" disabled="{{x.radio_dis}}">{{item.labtext+''}}</checkbox></view></block></checkbox-group></block><block tt:if="{{x.radio_display}}"><view style="width:195px;"><button style="width:70px;white-space:nowrap;text-align:center;height:30px;display:flex;align-items:center;justify-content:center;font-size:15px;" disabled="{{x.radio_dis}}" data-idx="{{i}}" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">确定</button></view></block><block tt:if="{{x.confirm_display}}"><view style="width:195px;"><button data-event-opts="{{[['tap',[['myConfirm',['$event']]]]]}}" style="width:70px;white-space:nowrap;text-align:center;height:30px;display:flex;align-items:center;justify-content:center;font-size:15px;margin-top:30px;" bindtap="__e">咨询</button></view></block></view></view></view></block></view></block><block tt:if="{{msgLoad}}"><view class="cu-item"><image class="cu-avatar" src="/static/images/robt.png" mode="aspectFill"></image><view class="main"><view class="contentload bg-white"><view><view class="loading"></view><view class="loading"></view><view class="loading"></view></view></view></view></view></block></view><view style="height:200rpx;"></view></scroll-view><view class="search" style="{{'padding-bottom:'+(inputBottom+'px')+';'}}"><input class="search_input" maxlength="99999" type="text" confirm-type="search" placeholder-class="search_input_placehold" placeholder="请描述您的问题" adjust-position="{{false}}" data-event-opts="{{[['confirm',[['sendMsg',[true]]]],['focus',[['inputBindFocus',['$event']]]],['blur',[['inputBindBlur',['$event']]]],['input',[['__set_model',['','msg','$event',[]]]]]]}}" value="{{msg}}" bindconfirm="__e" bindfocus="__e" bindblur="__e" bindinput="__e"/><block tt:if="{{msgLoad===false}}"><view><button class="bg-cyan search_button" disabled="{{msgLoad}}" data-event-opts="{{[['tap',[['sendMsg',[true]]]]]}}" bindtap="__e"><image style="width:30rpx;height:30rpx;" src="/static/images/send.png"></image></button></view></block><block tt:if="{{msgLoad===true}}"><view class="bg-cyan search_button2"><view class="loading"></view><view class="loading"></view><view class="loading"></view></view></block></view></view>